{
  "name": "xpbl4/yii2-assets",
  "description": "The reusable assets for Yii2 framework.",
  "type": "yii2-extension",
  "keywords": ["yii2", "extension", "widget", "assets"],
  "homepage": "https://github.com/Xpbl4/yii2-assets",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Serge Mashkov",
      "email": "xpbl4@motorwars.ru",
      "homepage": "https://github.com/Xpbl4",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/Xpbl4/yii2-assets/issues?state=open",
    "source": "https://github.com/Xpbl4/yii2-assets"
  },
  "repositories":[
    {
      "type": "git",
      "url": "https://github.com/Xpbl4/yii2-assets.git"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
